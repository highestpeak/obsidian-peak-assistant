	"description": "Peak Assistant is a plugin for Obsidian (https://obsidian.md) that allows you to use AI to help you with your notes.",
	"main": "main.js",
	"scripts": {
		"dev:tailwind": "npx tailwindcss -i src/styles/tailwind.css -o styles.css --watch",
		"dev:ts": "node esbuild.config.mjs",
		"dev": "npm-run-all --parallel dev:*",
		"build:tailwind": "npx tailwindcss -i src/styles/tailwind.css -o styles.css --minify",
		"build:css": "npm run build:tailwind",
		"build": "npm run build:css && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
