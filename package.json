{
	"name": "obsidian-sample-plugin",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev:tailwind": "npx tailwindcss -i src/styles/tailwind.css -o styles.css --watch",
		"dev:ts": "node esbuild.config.mjs",
		"dev": "npm-run-all --parallel dev:*",
		"build:tailwind": "npx tailwindcss -i src/styles/tailwind.css -o styles.css --minify",
		"build:css": "npm run build:tailwind",
		"build": "npm run build:css && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@lobehub/icons": "^2.44.0",
		"@radix-ui/react-dialog": "^1.0.5",
		"@radix-ui/react-slot": "^1.0.2",
		"autoprefixer": "^10.4.16",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"gray-matter": "^4.0.3",
		"lucide": "^0.553.0",
		"lucide-react": "^0.555.0",
		"marked": "^15.0.11",
		"moment": "^2.30.1",
		"pdfjs-dist": "^5.4.394",
		"postcss": "^8.4.32",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.48.2",
		"simple-git": "^3.27.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^2.1.0",
		"tailwindcss": "^3.4.1",
		"uuid": "^9.0.1",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^16.11.6",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.27.0",
		"esbuild-plugin-alias": "^0.2.1",
		"npm-run-all": "^4.1.5",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
